          emissiveColor 0.0 0.0 0.0
          shininess 0.5
          transparency 0.0
        }
      }
      geometry IndexedFaceSet {
        normalPerVertex TRUE
        coord Coordinate { point [
          {CX}   {DY}   0.05,
          {CX ~} {DY}   0.05,
          {CX ~} {DY ~} 0.05,
          {CX}   {DY ~} 0.05,
          {CX}   {DY}   {BH},
          {CX ~} {DY}   {BH},
          {CX ~} {DY ~} {BH},
          {CX}   {DY ~} {BH} ] }
        coordIndex [
          3, 2, 1, 0, -1,
          4, 5, 6, 7, -1,
          0, 4, 7, 3, -1,
          1, 5, 4, 0, -1,
          2, 6, 5, 1, -1,
