        material Material {
          diffuseColor 0.5 0.3 0.2
          specularColor 0.5 0.3 0.2
          emissiveColor 0.0 0.0 0.0
          ambientIntensity 0.9
          transparency 0.0
          shininess 1.0
        }
      }
      geometry IndexedFaceSet {
        {CX ~ @X1   X1 CX 0.3 * + @X2   BH 0.05 + @YBAR}
        normalPerVertex TRUE
        coord Coordinate { point [
          {X2} {DY}   {YBAR},
          {X1} {DY}   {YBAR},
          {X1} {DY ~} {YBAR},
          {X2} {DY ~} {YBAR} ] }
